    "alert.html": {
        "title": "Alert",
        "header_link": "{{ HOSTNAME_PROTOCOL }}",
        "alert_message": "{{ message }}",
        "alert_flight-number": "{{ tracker.carrier.airline.code }}{{ tracker.carrier.number }} {% trans 'of' %}",
        "alert_flight-airline": "{{ tracker.carrier.airline.name }}",
        "alert_flight-shared": "{% if tracker.user.email != recipient.identity %}{% trans 'Shared by' %} {{ user.get_full_name_or_email }}{% endif %}",
        "alert_origin": "{% trans 'Departure' %}",
        "alert_ori-airport-code": "{{ tracker.carrier.flight.origin.code }}",
        "alert_ori-terminal-code": "{{ tracker.carrier.flight.departure_terminal|default:'-' }}",
        "alert_ori-city-name": "{{ tracker.carrier.flight.origin.city }}, ",
        "alert_ori-city-code": "{{ tracker.carrier.flight.origin.country_code }}",
        "alert_ori-date": "{{ tracker.carrier.flight.departure_local_time|date:user.get_profile.get_date_format_web }}",
        "alert_ori-time": "{{ tracker.carrier.flight.departure_local_time|time:user.get_profile.get_time_format }} {{ tracker.carrier.flight.origin.country_code }}T",
        "alert_ori-terminal-text": "{% trans 'Terminal' %}",
        "alert_ori-terminal-split-text": "term",
        "alert_ori-gate-text": "{% trans 'Gate' %}",
        "alert_ori-terminal-value": "{{ tracker.carrier.flight.departure_terminal|default:'-' }}",
        "alert_ori-gate-value": "{{ tracker.carrier.flight.departure_gate|default:'-' }}",
        "alert_destination": "{% trans 'Arrival' %}",
        "alert_des-airport-code": "{{ tracker.carrier.flight.destination.code }}",
        "alert_des-terminal-code": "{{ tracker.carrier.flight.arrival_terminal|default:'-' }}",
        "alert_des-city-name": "{{ tracker.carrier.flight.destination.city }}, ",
        "alert_des-city-code": "{{ tracker.carrier.flight.destination.country_code }}",
        "alert_des-date": "{{ tracker.carrier.flight.arrival_local_time|date:user.get_profile.get_date_format_web }}",
        "alert_des-time": "{{ tracker.carrier.flight.arrival_local_time|time:user.get_profile.get_time_format }}{% if tracker.carrier.flight.diverted_airport %} {{ tracker.carrier.flight.diverted_airport.country_code }}T{% else %} {{ tracker.carrier.flight.destination.country_code }}T{% endif %}",
        "alert_des-terminal-text": "{% trans 'Terminal' %}",
        "alert_des-terminal-split-text": "term",
        "alert_des-gate-text": "{% trans 'Gate' %}",
        "alert_des-band-text": "{% trans 'Baggage' %}",
        "alert_des-terminal-value": "{{ tracker.carrier.flight.arrival_terminal|default:'-' }}",
        "alert_des-gate-value": "{{ tracker.carrier.flight.arrival_gate|default:'-' }}",
        "alert_des-band-value": "{{ tracker.carrier.flight.baggage|default:'-' }}",
        "alert_status-duration_title": "{% trans 'Duration' %}: ",
        "alert_status-duration_time": "{{ tracker.carrier.flight.duration|to_hours_and_minutes }}",
        "alert_status-percentage": "({{ tracker.carrier.flight.percentage|floatformat }}%)",
        "alert_status-title": "{% trans 'Status' %}: ",
        "alert_status-display": "{{ tracker.carrier.flight.get_status_display }}",
        "alert_unsubscribe-text-1": "{% if tracker.user.id != user.id %}{% trans 'I do not want to receive Flight Status Alerts from FyersConcierge' %}.{% endif %}",
        "alert_unsubscribe-text-2": "{% if tracker.user.id != user.id %}{% trans 'I do not want to receive Flight Status Alerts from' %} {{ user.first_name }} {{ user.last_name }} ({{ user.email }}).{% endif %}",
